<h1>List Of All Tutor Ads</h1>
<%= form_tag(tutor_ads_path, method: "get") do%>
	<%= label_tag(:query,"Search For:")%>
	<%= text_field_tag(:query, "Chemistry Tutor, etc.") %>
	<%= submit_tag("Seach")%>
<%end%>
<table>
	<tr>
		<th>Title</th>
		<th>Offering/Wanted</th>
		<th>Description</th>
		<th>Rate</th>
		<th></th>
	</tr>
	<% @tutor_ads.each do |tutor_ad| %>
		<tr>
			<td><%= tutor_ad.title %></td>
			<td><%= tutor_ad.offering %></td>
			<td><%= tutor_ad.description %></td>
			<td><%= tutor_ad.rate %></td>
		</tr>
	<%end%>
</table>
