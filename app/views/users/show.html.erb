<h1> User Profile </h1>

<table>
  <tr>
    <th>First Name</th>
    <th><%= @user.first_name %></th>
  </tr>
  <tr>
    <th>Last Name</th>
    <th><%= @user.last_name %></th>
  </tr>
  <tr>
    <th>Email</th>
    <th><%= @user.email %></th>
  </tr>
</table>

<%= link_to 'Edit profile', edit_user_path %>
